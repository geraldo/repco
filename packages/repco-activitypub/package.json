{
  "name": "repco-activitypub",
  "version": "0.1.0",
  "license": "MIT OR Apache-2.0",
  "maintainers": [
    "repco contributors"
  ],
  "main": "./dist/src/server.js",
  "type": "module",
  "scripts": {
    "build": "yarn prisma generate && tsc --build",
    "watch": "tsc --build -w",
    "dev": "run-p watch start:watch",
    "docs": "typedoc src/mod.ts --out ./docs",
    "start": "node dist/bin.js",
    "start:watch": "nodemon -r source-map-support/register --watch dist --watch ../repco-core/dist dist/bin.js",
    "prisma": "dotenv -e ../../.env -- prisma",
    "test": "brittle dist/test/*.js"
  },
  "dependencies": {
    "@digitalbazaar/http-digest-header": "^2.0.0",
    "activitypub-http-signatures": "^2.0.1",
    "body-parser": "^1.18.3",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "pino-http": "^8.3.3",
    "prisma": "^4.12.0",
    "@prisma/client": "^4.12.0",
    "repco-common": "*",
    "repco-prisma": "*",
    "source-map-support": "^0.5.21",
    "undici": "^5.27.2",
    "zod": "^3.19",
    "zod-error": "^1.5.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13"
  }
}
