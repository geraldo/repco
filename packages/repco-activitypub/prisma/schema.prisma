datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["deno"]
}

model ApLocalActor {
  name String @unique
  keypair Json
}

model ApFollowedActors {
  localName String
  remoteId String
  @@unique([localName, remoteId])
}

model ApMessages {
  id String @unique
  actorId String
  type String
  objectId String
  receivedAt DateTime
  details Json
}
